Test-Scenario-1:
  id: ts_01
  path: tests/hydratk/lib/array/test_operation.jedi
  name: test_operation
  desc: Unit tests for module hydratk/lib/array/operation.py
  author: Petr Rasek <bowman@hydratk.org>
  version: 1.0
  
  pre-req: |
    import hydratk.lib.array.operation as op  
    
  Test-Case-1:
    id: tc_01
    name: subdict
    desc: Tests for method subdict
  
    Test-Condition-1: 
      id: tco_01
      name: one_elem
      desc: Get one element from dict
      test: |
        o_dict = {'aa': 1, 'bb': 2, 'cc': 3, 'dd': 4}
        subset = ['bb']
        subdict = op.subdict(o_dict, subset) 
                         
      validate: |  
        expected = {'bb': 2} 
        this.test_result = (subdict == expected)
        assert (this.test_result == True), 'current:{0}, expected:{1}'.format(subdict, expected)    
        
    Test-Condition-2: 
      id: tco_02
      name: two_elems
      desc: Get two elements from dict
      test: |
        subset = ['cc', 'aa']
        subdict = op.subdict(o_dict, subset) 
                         
      validate: |  
        expected = {'aa': 1, 'cc': 3} 
        this.test_result = (subdict == expected)
        assert (this.test_result == True), 'current:{0}, expected:{1}'.format(subdict, expected)      
        
    Test-Condition-3: 
      id: tco_03
      name: all_elems
      desc: Get all elements from dict
      test: |
        subset = o_dict.keys()
        subdict = op.subdict(o_dict, subset) 
                         
      validate: |  
        expected = o_dict
        this.test_result = (subdict == expected)
        assert (this.test_result == True), 'current:{0}, expected:{1}'.format(subdict, expected)    
        
    Test-Condition-4: 
      id: tco_04
      name: no_elems
      desc: Get no elements from dict
      test: |
        subset = []
        subdict = op.subdict(o_dict, subset) 
                         
      validate: |  
        expected = {}
        this.test_result = (subdict == expected)
        assert (this.test_result == True), 'current:{0}, expected:{1}'.format(subdict, expected)     
        
    Test-Condition-5: 
      id: tco_05
      name: unknown_elem
      desc: Get unknown element from dict
      test: |
        subset = ['ee']
        subdict = op.subdict(o_dict, subset) 
                         
      validate: |  
        expected = {}
        this.test_result = (subdict == expected)
        assert (this.test_result == True), 'current:{0}, expected:{1}'.format(subdict, expected) 
        
    Test-Condition-6: 
      id: tco_06
      name: combined_elems
      desc: Get combination of known/unknown elements from dict
      test: |
        subset = ['cc', 'ee', 'aa', 'ff']
        subdict = op.subdict(o_dict, subset) 
                         
      validate: |  
        expected = {'aa': 1, 'cc': 3}
        this.test_result = (subdict == expected)
        assert (this.test_result == True), 'current:{0}, expected:{1}'.format(subdict, expected)                                                     